<?xml version='1.0' encoding='UTF-8'?>
<model>
   <entity name="Playlist" sync="YES">
      <attribute name="title" type="STRING" textindex="YES" index="NO" />
   </entity>
   <entity name="Song" sync="YES">
      <attribute name="title" type="STRING" textindex="YES" index="NO" />
      <attribute name="seconds" type="UINT32" textindex="NO" index="NO" />
   </entity>
   <relationship name="songs" origin="Playlist" destination="Song" type="ORDERED-TO-MANY" delete="NULLIFY"  />
   <relationship name="containingPlaylists" origin="Song" destination="Playlist" type="UNORDERED-TO-MANY" delete="NULLIFY" inverse="songs" />
</model>